  // jump to open file if already opened
  "workbench.editor.revealIfOpen": true,
  // python file uses black formatter
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  //   "python.formatting.blackArgs": ["--line-length", "120"],
  // black formatter args
  "black-formatter.args": [
    "--line-length",
    "120"
  ],
  // Python/Pylance or linter configurations
  // pylance: specify python version under a virtualenv
  "python.defaultInterpreterPath": ".\\winEnv\\Scripts\\python.exe",
  // "python.defaultInterpreterPath": "${env:HOME}/.pyenv/versions/3.11.8/envs/xdata-aicor-engine-api.git/bin/python3",
  "python.analysis.typeCheckingMode": "basic",
  // pylance: extra paths for import resolution
  "python.analysis.extraPaths": [
    "./src/lib"
  ],
  // plyance: exclude paths
  "python.analysis.exclude": [
    "./out",
    "./temp",
    // Avoid strange warnings because of packages installed on virtualenv
    "./winEnv"
  ],
  // "python.autoComplete.extraPaths": [
  //   "/usr/local/lib/python3.8/dist-packages/cv2/python-3.8"
  // ],
  // auto format
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "scm.showHistoryGraph": false,
  // Rest client
  "rest-client.previewResponseInUntitledDocument": true
